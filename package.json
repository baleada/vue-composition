{
  "name": "@baleada/composition",
  "version": "0.0.0",
  "description": "Functions that expose @baleada/logic libraries for component composition in front-end frameworks",
  "main": "lib/index.js",
  "scripts": {
    "gen-vue": "node scripts/generateFramework.js 'vue'",
    "gen-react": "node scripts/generateFramework.js 'react'",
    "gen-svelte": "node scripts/generateFramework.js 'svelte'",
    "gen-all": "npm run gen-vue && npm run gen-react && npm run gen-svelte",
    "empty-vue": "node scripts/emptyDir.js 'vue'",
    "empty-react": "node scripts/emptyDir.js 'react'",
    "empty-svelte": "node scripts/emptyDir.js 'svelte'",
    "empty-lib": "node scripts/emptyDir.js 'lib'",
    "empty-all": "npm run empty-vue && npm run empty-react && npm run empty-svelte && npm run empty-lib",
    "babelify-vue": "npx babel src/vue --out-dir vue",
    "babelify-react": "npx babel src/react --out-dir react",
    "babelify-svelte": "npx babel src/svelte --out-dir svelte",
    "babelify-index": "npx babel src/index.js --out-file lib/index.js",
    "babelify-all": "npm run babelify-vue && npm run babelify-react && npm run babelify-svelte && npm run babelify-index",
    "prepare": "npm run gen-all && npm run empty-all && npm run babelify-all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/baleada/composition.git"
  },
  "keywords": [
    "baleada",
    "vue composition function",
    "react hook",
    "svelte hook"
  ],
  "author": {
    "name": "Alex Vipond",
    "email": "hello@alexvipond.dev",
    "url": "https://alexvipond.dev"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/baleada/composition/issues"
  },
  "homepage": "https://gitlab.com/baleada/composition#readme",
  "dependencies": {
    "@baleada/logic": "0.0.0",
    "on-change": "^1.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-private-methods": "^7.6.0",
    "@babel/preset-env": "^7.6.0"
  },
  "sideEffects": "false"
}
